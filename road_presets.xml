<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0" author="Megha" version="" shortdescription="" description="">
 <group name = "Transportation Data" type ="way,node">
    <item name="Road" icon="presets/empty.png" type="way">
        <multiselect key="highway" text="Highway type" values="trunk;primary;secondary;residential;footway;path;service;pedestrian;track;construction;proposed"/>
      <multiselect key="class" text="Road Class" values="DCRN;VCRN"/>
   <multiselect key="access_status" text="Access Status" values="all_weather;fair_weather"/>
          <multiselect key="surface" text="Pavement Type" values="premix carpet;earth;gravel;DBST;ottaseal;asphalt concrete;concrete"/>
    <text key="width" text="Pavement Width"/>
    <text key="width:ROW" text="ROW Width"/>
   
    <text key="width:shoulder" text="One side Width"/>
   <multiselect text="Road Condition" key="condition" values="good;fair;poor"/>
         <text text="Road Code" key="ref"/>
    <text text="Start Chainage" key="start_chainage"/>
        <text key="lanes" text="No. of Lanes"/> 
         <text key="maxspeed" text="Maxspeed"/>
    <text key="name" text="Name"/>
<text key="resurfaced:date" text="Resurfaced Date"/>
    <text text="Survey Year" key="survey:year"/>
    <text text="Survey Date" key="survey:date"/>
    <text text="Surveyor Name" key="surveyor:name"/>
   <key key="source" value="ILO transportation mapping"/>
        <key key="project:url" value ="https://github.com/MeghaStha/ILO-training"/>
     </item>
    <item name="Causeways" icon="presets/empty.png" type="way,node">
    <key key="causeway" value="yes"/>
      <text key="length" text ="Length"/>
      <text key="span" text="Span Length"/>
    <multiselect key="causeway:type" text="Causeway Type" values="RCC causeway;Dry stone"/>
   <key key="source" value="ILO transportation mapping"/>
        <key key="project:url" value ="https://github.com/MeghaStha/ILO-training"/>
     </item>   
    <item name="Bridge" icon="presets/empty.png" type="way,node">
    <key key="bridge" value="yes"/>
      <text key="length" text ="Length"/>
      <text key="span" text="Span Length"/>
    <multiselect key="bridge:type" text="Bridge Type" values="RCC;Steel girder;Arch;Steel and RCC composite girder;Motorable"/>
    <key key="source" value="ILO transportation mapping"/>
        <key key="project:url" value ="https://github.com/MeghaStha/ILO-training"/>
     </item>
  <item name="Culvert" icon="presets/empty.png" type="way,node">
    <key key="layer" value="-1"/>
    <key key="tunnel" value="culvert"/>
      <text key="length" text ="Length"/>
      <text key="span" text="Span Length"/>
    <multiselect key="culvert:type" text="Culvert Type" values="Hume pipe;RCC slab;slab"/>
    <key key="source" value="ILO transportation mapping"/>
     <key key="project:url" value ="https://github.com/MeghaStha/ILO-training"/>
     </item>
       <item name="Bus stop" icon="presets/empty.png" type="node">
     <text key="name" text ="Name of stop"/>
    <key key="source" value="ILO transportation mapping"/>
    <key key="project:url" value ="https://github.com/MeghaStha/ILO-training"/>
     </item>
    </group>
</presets>
